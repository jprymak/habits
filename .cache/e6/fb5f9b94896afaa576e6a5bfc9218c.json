\r\n        case 10:\r\n          this.name = \"Listopad\";\r\n          this.days = 30;\r\n          break;\r\n      }\r\n    }\r\n  \r\n    createContent() {\r\n      this.verifyMonth();\r\n      let content = \"\";\r\n      for (let i = 1; i <= this.days; i++) {\r\n        const date = new Date(2020, this.monthNumber, i);\r\n        content += `<div class='day'>\r\n        <p class='day__number'>${date.getDate()}</p>\r\n        <p class='day__name'>${checkDayName(date.getDay())}</p>\r\n        <div class=\"habit-status\"></div></div>`;\r\n      }\r\n      const month = document.createElement(\"div\");\r\n      month.classList.add(\"month\");\r\n      month.innerHTML = `\r\n      <div class=\"month__info\">\r\n     <span class=\"month__name\">${this.name}</span>\r\n     <span class=\"month__habits\">0/${this.days}</span>\r\n     </div>\r\n     <div class='month__days'>${content}</div>\r\n      `;\r\n      return month;\r\n    }\r\n  }\r\n  \r\n  function checkDayName(day) {\r\n    switch (day) {\r\n      case 0:\r\n        return \"nd\";\r\n      case 1:\r\n        return \"pon\";\r\n      case 2:\r\n        return \"wt\";\r\n      case 3:\r\n        return \"śr\";\r\n      case 4:\r\n        return \"czw\";\r\n      case 5:\r\n        return \"pt\";\r\n      case 6:\r\n        return \"sob\";\r\n    }\r\n  }\r\n  \r\n  export default Month;\r\n  "},"lineCount":null}},"error":null,"hash":"95ced67073f95580dbf1531b23a1cd3a","cacheData":{"env":{}}}